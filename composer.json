{
    "require": {
        "php": "^7.2",
        "ramsey/uuid": "^3.9",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/messenger": "^4.3",
        "symfony/serializer-pack": "^1.0",
        "symfony/validator": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.5",
        "phpstan/phpstan": "^0.11.16",
        "phpunit/phpunit": "^8.5",
        "rector/rector": "^0.5.13",
        "symfony/web-server-bundle": "^4.3",
        "symplify/easy-coding-standard": "^6.1",
        "thecodingmachine/phpstan-strict-rules": "^0.11.2",
        "vimeo/psalm": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs-fixer": "bin/php-cs-fixer fix --config=.php_cs.dist",
        "phpstan": "phpstan analyse -c phpstan.neon --level max src",
        "psalm": "psalm",
        "cs-check": "ecs check src",
        "rector": "rector process src -c rector.yaml -s code-quality",
        "rector-solid": "rector process src -c rector.yaml -s solid"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
