{
    "require": {
        "php": "^7.4",
        "enqueue/rdkafka": "^0.10",
        "ramsey/uuid": "^4.1",
        "sensio/framework-extra-bundle": "^5.6",
        "sroze/messenger-enqueue-transport": "^0.5.0",
        "symfony/console": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/flex": "^1.9",
        "symfony/framework-bundle": "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/messenger": "^5.1",
        "symfony/monolog-bundle": "^3.5",
        "symfony/serializer-pack": "^1.0",
        "symfony/validator": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.42",
        "phpunit/phpunit": "^9.3",
        "rector/rector": "^0.7",
        "sensiolabs-de/deptrac-shim": "^0.8.2",
        "symfony/profiler-pack": "^1.0",
        "vimeo/psalm": "^3.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs-fixer": "php-cs-fixer fix --config=.php_cs.dist",
        "phpstan": "phpstan analyse -c phpstan.neon --level max src",
        "psalm": "psalm",
        "cs-check": "ecs check src",
        "rector": "rector process src -c rector.yaml"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.1.*"
        }
    }
}
