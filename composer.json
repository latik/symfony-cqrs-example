{
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/mongodb-odm-bundle": "^4.4",
        "doctrine/orm": "^2.10",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^1.8",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/amqp-messenger": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.5",
        "qossmic/deptrac-shim": "^0.11.1",
        "rector/rector": "^0.12.5",
        "symfony/stopwatch": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/web-profiler-bundle": "5.4.*",
        "vimeo/psalm": "^4.14"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs-fixer": "php-cs-fixer fix --verbose",
        "phpstan": "phpstan analyse -c phpstan.neon --level max src",
        "psalm": "psalm",
        "cs-check": "ecs check src",
        "rector": "rector process src"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
