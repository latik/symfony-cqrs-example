{
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/mongodb-odm-bundle": "^4.4",
        "doctrine/orm": "^2.10",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^1.8",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/amqp-messenger": "^6.0",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.0.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "6.0.*",
        "symfony/property-info": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/validator": "6.0.*",
        "symfony/yaml": "6.0.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.5",
        "qossmic/deptrac-shim": "^0.18.0",
        "rector/rector": "^0.12.10",
        "symfony/stopwatch": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/web-profiler-bundle": "6.0.*",
        "vimeo/psalm": "^4.17"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs-fix": "php-cs-fixer fix --verbose",
        "phpstan": "phpstan analyse -c phpstan.neon --level max src",
        "psalm": "psalm",
        "cs-check": "ecs check src",
        "rector": "rector process src"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.0.*",
            "docker": true
        }
    }
}
